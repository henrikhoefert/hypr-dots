browser mpv
#browser
macro b set browser librewolf    ; open-in-browser                  ; set browser mpv
#yank
macro y set browser "wl-copy %u" ; open-in-browser-noninteractively ; set browser mpv
#yt-dlp
macro d set browser "yt-dlp -o '~/Videos/%(title)s.%(ext)s' '%u'" ; open-in-browser-noninteractively ; set browser mpv
